{
  "name": "onliops-platform",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "20.x"
  },
  "packageManager": "npm@10.8.1",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "check": "tsc -b --noEmit",
    "smoke:test": "powershell -NoProfile -ExecutionPolicy Bypass -File tests/smoke.ps1",
    "seed:admin": "node scripts/seed-admin.ts",
    "validate:auth": "node scripts/validate-auth.ts",
    "setup:postgres": "bash scripts/setup-postgres.sh --db-name network_platform --db-user network_admin --db-pass $PGPASSWORD",
    "migrate:local": "bash scripts/apply-migrations.sh",
    "setup:nginx": "bash scripts/setup-nginx.sh --server-name local.project --site-root /opt/calabasas/dist",
    "monitor:services": "bash scripts/monitor/check-services.sh",
    "backup:db": "bash scripts/backup-postgres.sh"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "@reduxjs/toolkit": "^2.11.0",
    "@supabase/supabase-js": "^2.84.0",
    "@types/d3": "^7.4.3",
    "@types/three": "^0.181.0",
    "axios": "^1.13.2",
    "chart.js": "^4.5.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "d3": "^7.9.0",
    "date-fns": "^4.1.0",
    "express": "^5.2.1",
    "lucide-react": "^0.511.0",
    "notistack": "^3.0.2",
    "papaparse": "^5.5.3",
    "pdf-parse": "^2.4.5",
    "pg": "^8.16.3",
    "prom-client": "^15.1.3",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.8",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.3.0",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.0.2",
    "three": "^0.181.2",
    "web-vitals": "^4.2.0",
    "xlsx": "^0.18.5",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/node": "^22.15.30",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "babel-plugin-react-dev-locator": "^1.0.0",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-plugin-pwa": "^1.2.0",
    "vite-tsconfig-paths": "^5.1.4",
    "workbox-window": "^7.4.0"
  }
}
